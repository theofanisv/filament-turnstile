{
    "name": "theofanisv/filament-turnstile",
    "description": "Cloudflare Turnstile integration for Filament v4 forms",
    "keywords": [
        "laravel",
        "filament",
        "turnstile",
        "cloudflare",
        "captcha",
        "form",
        "validation"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Theofanis Vardatsikos",
            "email": "vardtheo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "filament/filament": "^4.0",
        "illuminate/support": "^10.0|^11.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Theograms\\FilamentTurnstile\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Theograms\\FilamentTurnstile\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Theograms\\FilamentTurnstile\\FilamentTurnstileServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
